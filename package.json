{
  "name": "react-for-mobile",
  "version": "1.0.0",
  "description": "based on umi, dva and ant-mobile",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "buildProd": "npm run build",
    "functions:build": "npm run generateMock && netlify-lambda build ./lambda",
    "functions:run": "npm run generateMock && cross-env NODE_ENV=dev netlify-lambda serve ./lambda",
    "generateMock": "node ./scripts/generateMock",
    "lint": "eslint --ext .js src mock tests && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint:fix": "eslint --fix --ext .js src mock tests && stylelint --fix 'src/**/*.less' --syntax less",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint 'src/**/*.less' --syntax less",
    "prettier": "node ./scripts/prettier.js",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tslint": "npm run tslint:fix",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "antd-mobile": "^2.3.1",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.0",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "nzh": "^1.0.4",
    "path": "^0.12.7",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "rc-animate": "^2.6.0",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "umi": "^2.8.20",
    "umi-plugin-react": "^1.7.2",
    "umi-request": "^1.0.5"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.2.0",
    "jest-puppeteer": "^4.1.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.9.0",
    "lint-staged": "^8.1.1",
    "merge-umi-mock-data": "^1.0.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.17.0",
    "react-sticky": "^6.0.3",
    "serverless-http": "^2.0.1",
    "slash2": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^2.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.12.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
